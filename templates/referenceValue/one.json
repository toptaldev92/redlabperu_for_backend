{
  "status": true,
  "data": {
    "id": "{{#? data.id}}",
    "name": "{{#? data.name}}",
    "unit": "{{#? data.Unit}}",
    "methodology": "{{#? data.Method}}",
    "examGroup": "{{#? data.ExaminationGroup}}",
    "examinationReferenceValues": {
      "{{#?each this.examinationReferenceValues}}": {
        "id": "{{#? this.id}}",
        "name": "{{#? this.name}}"
      }
    }
  }
}
